<%
var data = jsonData;
-%>
<!DOCTYPE html>
<html lang="ja">
<head>
<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<title><%= data.name %>さん | 社員紹介</title>
<link rel="stylesheet" href="css/style.css">
<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css" rel="stylesheet">
<!--[if lt IE 9]>
<script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->
<script src="https://code.jquery.com/jquery-1.12.4.min.js" integrity="sha256-ZosEbRLbNQzLpnKIkEdrPv7lOy9C27hHQ+Xp8a4MxAQ=" crossorigin="anonymous"></script>
<script src="js/jquery.pjax.js"></script>
<script src="js/common.js"></script>
</head>
<body class="sample" id="<%= data.id %>">
    <article id="fresh__contents">
        <header>
           <h1 class="changeContent">PJAXでページを切り替える【<%= data.id %>】</h1>
        </header>
        <section class="changeContent">
           <div class="page-header">
              <h1><%= data.name %> <small><%= data.position %></span></small></h1>
           </div>
            <p class="thumbnail"><img src="img/<%= data.pic %>.jpg"></p>
            <p>所属：<%= data.company %></p>
            <p>趣味：<%= data.hobby %></p>
            <div class="pjaxLink">
               <% include nav %>
            </div>
        </section>
        <div class="list-group text-center">
            <a class="list-group-item toThePageTop" href="#"><span aria-hidden="true" class="glyphicon glyphicon-chevron-up"></span></a>
        </div>
    </article>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
</body>
</html>